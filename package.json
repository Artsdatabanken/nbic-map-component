{
  "name": "nbic-map-component",
  "version": "0.1.0",
  "description": "Shared map component built on OpenLayers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rimraf dist || rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"playground/**/*.{ts,tsx}\"",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "npm run build"
  },
  "keywords": [
    "openlayers",
    "map",
    "gis",
    "nbic",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ol": "^10.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
